<%= render "layouts/errors", :object => @post %>

<%= form_for @post do |f| %>
  <%= f.label :content %>
  <br>
  <%= f.text_field :content %>
  <br>
  <%= f.label :image %>
  <br>
  <%= f.text_field :image %>
  <%= f.hidden_field(:user_id, :value => current_user.id) %>
  <br>
  <%= f.submit %>
<% end %>
